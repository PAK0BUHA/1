Sub Clear_H()
    ActiveDocument.Tables(1).Rows(1).Cells(7).Select
    Selection.MoveRight Unit:=wdCharacter, Count:=5, Extend:=wdExtend
    Selection.Delete Unit:=wdCharacter, Count:=1
End Sub
